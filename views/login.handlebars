<div class="loginform">
  <div>
    <h2 class="header">Login</h2>
  </div>
  <form>
    <div class="form-group">
      <label for="email">Email</label>
      <input
        type="email"
        class="form-control"
        id="email"
        aria-describedby="email"
        placeholder="Enter email"
      />
    </div>
    <br />
    <div class="form-group">
      <label for="password">Password</label>
      <input
        type="password"
        class="form-control"
        id="password"
        placeholder="Password"
      />
    </div>
    <br />
    <br />
    <button
      type="submit"
      class="btn btn-dark loginbtn"
      id="logInBtn"
    >Submit</button>
  </form>
  <br />
  <small id="signupinfo" class="form-text text-muted">Dont have an account? Sign
    up!
  </small>
  <br />
  <div>
    <a href="/signup"><button class="btn btn-dark signupbtn">
        Sign Up!
      </button></a>
  </div>
  <script src="./js/userfeatures/login.js"></script>
</div>